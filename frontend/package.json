{
  "name": "llm-web",
  "version": "1.0.0",
  "description": "llm web project",
  "scripts": {
    "prepare": "cd .. && husky install frontend/.husky",
    "analyze": "cross-env ANALYZE=true vite build",
    "start": "ts-node ./scripts/guide.js",
    "start:supplier": "ts-node ./scripts/bootstrap.js dev --path src/apps/supplier",
    "start:operator": "ts-node ./scripts/bootstrap.js dev --path src/apps/operator",
    "start:login": "ts-node ./scripts/bootstrap.js dev --path src/apps/login",
    "build:login": "ts-node ./scripts/bootstrap.js build --path src/apps/login",
    "build:supplier": "ts-node ./scripts/bootstrap.js build --path src/apps/supplier",
    "build:operator": "ts-node./scripts/bootstrap.js build --path src/apps/operator",
    "build": "ts-node ./scripts/bootstrap.js build",
    "build:scripts": "rollup --config rollup.config.ts --bundleConfigAsCjs",
    "build:js": "node ./scripts/bootstrap.js build",
    "lint": "eslint src --fix --ext .js,.jsx,.ts,.tsx",
    "lint-staged": "lint-staged",
    "lint:script": "eslint --ext .js,.jsx,.ts,.tsx --fix --quiet ./src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/opendatalab/LabelLLM.git"
  },
  "keywords": [
    "llm"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@ahooksjs/use-url-state": "^3.5.1",
    "@ant-design/icons": "^5.0.1",
