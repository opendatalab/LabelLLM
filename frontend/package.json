    "axios": "^1.3.5",
    "class-variance-authority": "^0.5.2",
    "clsx": "^1.2.1",
    "dayjs": "^1.11.7",
    "diff": "^5.1.0",
    "diff2html": "^3.4.46",
    "history": "5.3.0",
    "html2canvas": "^1.4.1",
    "json-schema-library": "^7.4.8",
    "katex": "^0.16.7",
    "lodash": "^4.17.21",
    "qs": "^6.11.1",
